<?xml version="1.0" encoding="ISO-8859-1"?>
<openapi:API xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:openapi="platform:/resource/TransformationRules/Openapi.ecore" openapi="3.0.0">
  <info version="1" title="Data.csv" description="Obtaining the Data.csv"/>
  <servers url="www.urlprueba.com"/>
  <servers url="www.urlprueba2.com"/>
  <paths pattern="/">
    <operations name="get" summary="GET Data.csv" operationId="getData.csv" description="Use value 'all' in a parameter for non-empty values">
      <parameters name="stationname" in="query" description="stationname"/>
      <parameters name="linename" in="query" description="linename"/>
    </operations>
  </paths>
  <paths pattern="/stationname/{stationname}">
    <operations name="get" summary="GET stationname" operationId="getstationname" description="Use value 'all' in a parameter for non-empty values">
      <parameters name="stationname" in="path" description="stationname" required="true">
        <schema type="string"/>
      </parameters>
    </operations>
  </paths>
  <paths pattern="/linename/{linename}">
    <operations name="get" summary="GET linename" operationId="getlinename" description="Use value 'all' in a parameter for non-empty values">
      <parameters name="linename" in="path" description="linename" required="true">
        <schema type="string"/>
      </parameters>
      <responses code="200" description="successful operation" contentType="application/json">
        <schema type="array">
          <items ref="#/components/schemas/COLUMNS"/>
        </schema>
      </responses>
    </operations>
  </paths>
  <components>
    <schema type="object" required="stationname" title="COLUMNS" xml="COLUMNS">
      <properties name="stationname" type="string" example="Atocha"/>
      <properties name="linename" type="string" example="Linea 1"/>
    </schema>
  </components>
</openapi:API>
